{
    "env": {
        "browser": true,
        "node": true,
        "es2024": true
    },
    "extends": ["eslint:recommended", "prettier", "mocha"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    }, 
    "rules":{
        "no-shadow":"warn"
    }
}
